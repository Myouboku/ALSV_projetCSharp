<Window x:Class="Projetcsharp.AjoutPraticien"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projetcsharp"
        mc:Ignorable="d"
        Title="AjoutPraticien" Height="450" Width="800" Loaded="WindowsLoaded">
    <Grid>
        <Label Content="Nom&#xD;&#xA;" HorizontalAlignment="Left" Margin="76,54,0,0" VerticalAlignment="Top" Height="28" Width="67"/>
        <TextBox x:Name="txtboxNom" HorizontalAlignment="Left" Margin="135,59,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Prénom" HorizontalAlignment="Left" Margin="58,125,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtboxPrenom" HorizontalAlignment="Left" Margin="135,133,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Adresse :" HorizontalAlignment="Left" Margin="112,0,0,0" VerticalAlignment="Center"/>
        <Label Content="Num et libelle" HorizontalAlignment="Left" Margin="19,236,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtboxAdresse" HorizontalAlignment="Left" Margin="110,240,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Code Postal" HorizontalAlignment="Left" Margin="25,267,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtBoxCP" HorizontalAlignment="Left" Margin="110,275,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" MaxLength="5" TextChanged="txtBoxCP_TextChanged"/>
        <TextBox x:Name="txtBoxVille" HorizontalAlignment="Left" Margin="110,307,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Ville" HorizontalAlignment="Left" Margin="45,303,0,0" VerticalAlignment="Top"/>
        <Label Content="Discipline" HorizontalAlignment="Left" Margin="294,191,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cbBoxDiscipline" HorizontalAlignment="Left" Margin="373,196,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnAjoutPraticien" Content="Ajout Praticien" HorizontalAlignment="Left" Margin="323,328,0,0" VerticalAlignment="Top" Height="48" Width="170" Click="btnAjoutPraticien_Click"/>
    </Grid>
</Window>
